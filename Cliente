package org.example;

public class Cliente extends Persona {
    private String destino;


    public String getDestino() {
        return this.destino;
    }

    public void setDestino(String destino) {
        this.destino = destino;
    }


    public Cliente(String nombre, String apellido, String segundoApellido, String rut, String correoElectronico, int edad, String destino) {
        super(nombre, apellido, segundoApellido, rut, correoElectronico, edad);
        this.destino = destino;
    }

    public String toString() {
        return super.nombre + "," + super.apellido + "," + super.segundoApellido + "," + super.rut + "," + super.correoElectronico + "," + super.edad + "," + this.destino;
    }


   }
