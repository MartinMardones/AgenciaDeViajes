package org.example;

public class Vendedor {
    private String nombreV;
    private String apellidoV;
    private String segundoApellidoV;
    private String rutV;
    private String correoElectronicoV;



    public String getNombre() {
        return this.nombreV;
    }

    public void setNombre(String nombre) {
        this.nombreV = nombre;
    }

    public String getApellido() {
        return this.apellidoV;
    }

    public void setApellido(String apellido) {
        this.apellidoV = apellido;
    }

    public String getSegundoApellido() {
        return this.segundoApellidoV;
    }

    public void setSegundoApellido(String segundoApellido) {
        this.segundoApellidoV = segundoApellido;
    }

    public String getRut() {
        return this.rutV;
    }

    public void setRut(String rut) {
        this.rutV = rut;
    }


    public String getCorreoElectronico() {
        return this.correoElectronicoV;
    }

    public void setCorreoElectronico(String correoElectronico) {
        this.correoElectronicoV = correoElectronico;
    }

    public Vendedor(String nombre, String apellido, String rut, String correoElectronico, String electronico) {
        this.nombreV = nombre;
        this.apellidoV = apellido;
        this.segundoApellidoV = segundoApellidoV;
        this.rutV = rut;
        this.correoElectronicoV = correoElectronico;
    }

    public String toString(){
        return "Nombre: "+this.nombreV+" "+this.apellidoV+" "+this.segundoApellidoV+", rut: "+this.rutV+", correo electronico "+this.correoElectronicoV;
    }


}
